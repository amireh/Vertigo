system PUMediaPack/Leaves
{
    category                                    Nature
    technique 
    {
        material                                PUMediaPack/Leave
        renderer                                Billboard
        {
        }
        emitter                                 Line
        {
            emission_rate                       30
            angle                               10
            time_to_live                        15
            velocity                            dyn_random
            {
                min                             50
                max                             200
            }
            all_particle_dimensions             dyn_random
            {
                min                             100
                max                             250
            }
            position                            -800 600 0
            direction                           0 -1 0
            end                                 2400 600 0
        }
        affector                                TextureRotator 
        {
            use_own_rotation                    true
            rotation                            dyn_random
            {
                min                             0
                max                             180
            }
            rotation_speed                      dyn_random
            {
                min                             -1
                max                             1
            }
        }
        affector                                LinearForce WindLeft
        {
            enabled                             false
            force_vector                        -150 -50 0
        }
        affector                                LinearForce WindRight
        {
            enabled                             false
            force_vector                        50 0 0
        }
        observer                                OnRandom 
        {
            observe_interval                    1
            handler                             DoEnableComponent 
            {
                enable_component                affector_component WindLeft true
            }
            handler                             DoEnableComponent 
            {
                enable_component                affector_component WindRight false
            }
        }
        observer                                OnRandom 
        {
            observe_interval                    1
            handler                             DoEnableComponent 
            {
                enable_component                affector_component WindRight true
            }
            handler                             DoEnableComponent 
            {
                enable_component                affector_component WindLeft false
            }
            random_threshold                    0.6
        }
    }
}


