alias                                           PUMediaPack/LightningBolt/PointEmitter
{
    emitter                                     Point
    {
        enabled                                 false
        all_particle_dimensions                 dyn_random
        {
            min                                 256
            max                                 512
        }
        emission_rate                           20
        angle                                   360
        direction                               0 1 0
        velocity                                0.001
        colour                                  0.46 0.06 0.82
        duration                                2.5
        repeat_delay                            2.5
        time_to_live                            dyn_random
        {
            min                                 0.05
            max                                 0.15
        }
    }
}

